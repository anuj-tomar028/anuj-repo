<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="custom_fleet_vehicle_model_category_tree_view" model="ir.ui.view">
    <field name="name">custom.fleet.vehicle.model.category.tree.view</field>
    <field name="model">fleet.vehicle.model.category</field>
    <field name="inherit_id" ref="fleet.fleet_vehicle_model_category_view_tree"/>
    <field name="arch" type="xml">
        <xpath expr="//tree" position="inside">
            <field name="max_weight"/>
            <field name="max_volume"/>
        </xpath>
    </field>
    </record>
</odoo>
